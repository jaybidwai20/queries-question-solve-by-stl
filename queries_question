#include<bits/stdc++.h>
#include <iostream>

using namespace std;

int main()
{
    
    set<int>s;
    int n;
    cin>>n;
    while(n--)
    {
        int x,y;
        cin>>x>>y;
        if(x==1)
        {
            s.insert(y);
        }
        else if(x==2)
        {
            s.erase(y);
        }
        else
        {
            if(s.find(y)==s.end())
            {
                cout<<"No"<<endl;
            }
            else
            {
                cout<<"Yes"<<endl;
            }
        }
        
    }
}
